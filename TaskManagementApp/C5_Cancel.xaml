        xmlns:local="clr-namespace:TaskManagementApp"
        mc:Ignorable="d"
        Title="C5_Cancel" Height="300" Width="600" WindowStyle="None">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="179,64,0,0" TextWrapping="Wrap" Text="キャンセルしますか？" FontSize="30" VerticalAlignment="Top" Height="47" Width="329"/>
        <Button x:Name="Yes" Content="はい" FontSize="25" HorizontalAlignment="Left" Margin="106,212,0,0" VerticalAlignment="Top" Width="128" Height="38" Click="Yes_Click" Background="#FF68A46B" BorderBrush="{x:Null}" Foreground="White"/>
        <Button x:Name="No" Content="いいえ" FontSize="25" HorizontalAlignment="Left" Margin="354,212,0,0" VerticalAlignment="Top" Width="128" Height="38" Click="No_Click" Foreground="White" BorderBrush="{x:Null}" Background="#FF949991"/>

    </Grid>
